@Article{medaglia2012,
  Title                    = {{eParticipation research: Moving characterization forward (2006–2011)}},
  Author                   = {Medaglia, Rony},
  Journal                  = {Government Information Quarterly},
  Year                     = {2012},

  Month                    = jul,
  Number                   = {3},
  Pages                    = {346--360},
  Volume                   = {29},

  Abstract                 = {Research on the use of information technology to support democratic decision-making (eParticipation) is experiencing ongoing growth, stimulated by an increasing attention from both practitioner and research communities. This study provides the first longitudinal analysis of the development of the eParticipation field based on a shared framework, capturing the directions that the research field of eParticipation is taking in recent developments. Drawing on a literature search covering the period from April 2006 to March 2011, this study identifies, ana- lyzes, and classifies 122 research articles within the categories of eParticipation actors, activities, contextual factors, effects, evaluation, and methods. Findings show that the field has a high level of dynamism, as foci on eParticipation activities, contextual fac- tors, and effects have shifted in time, sometimes in counterintuitive directions. The study also suggests new analytical categories of research. Drawing on the analysis, inputs for a research agenda are suggested. These include the need to move beyond a technological perspective, encouraging the ongoing shift of research focus from government to citizens and other stakeholders, and the need to make eParticipation research methods more participatory in nature.},
  Doi                      = {10.1016/j.giq.2012.02.010},
  File                     = {:home/haydee/\'{A}rea de trabalho/Ref\_bib/Medaglia\_2012.pdf:pdf},
  ISSN                     = {0740624X},
  Keywords                 = {FAFICH,Literature review,eDemocracy,eGovernment,eParticipation,monografia,participa\c{c}\~{a}o digital},
  Mendeley-tags            = {FAFICH,monografia,participa\c{c}\~{a}o digital},
  Publisher                = {Elsevier Inc.},
  Url                      = {http://linkinghub.elsevier.com/retrieve/pii/S0740624X12000378}
}

@Article{MIGUEL2003,
  Title                    = {{Representa\c{c}\~{a}o Pol\'{i}tica em 3D: Elementos para uma TEoria Ampliada da Representa\c{c}\~{a}o Pol\'{i}tica}},
  Author                   = {Miguel, Luis Felipe},
  Journal                  = {Revista Brasileira de Ciencias Sociais},
  Year                     = {2003},
  Number                   = {51},
  Pages                    = {123--193},
  Volume                   = {18},

  Abstract                 = {O artigo discute os impasses e as al- ternativas para a representa\c{c}\~{a}o pol\'{i}- tica, tomando como ponto de parti- da a pol\^{e}mica sobre o conceito de poder, travada nos anos de 1960 e 1970 na ci\^{e}ncia pol\'{i}tica anglo-sax\~{a} e envolvendo, notadamente, Robert Dahl, Bachrach, Baratz e Steven Lu- kes. De acordo com o \'{u}ltimo, a compreens\~{a}o do poder deve levar em conta tr\^{e}s dimens\~{o}es: 1) a capa- cidade de tomar decis\~{o}es ou de vet\'{a}-las; 2) o controle sobre a agen- da, isto \'{e}, a determina\c{c}\~{a}o das ques- t\~{o}es que ser\~{a}o alvo de decis\~{a}o; e 3) a capacidade de anular o conflito social, impedindo que indiv\'{i}duos e grupos sociais tomem consci\^{e}ncia de seus verdadeiros interesses. Ape- sar dos problemas presentes nessa formula\c{c}\~{a}o, ela \'{e} \'{u}til para pensar a representa\c{c}\~{a}o pol\'{i}tica. As teorias predominantes levam em considera- \c{c}\~{a}o apenas a face mais evidente da representa\c{c}\~{a}o pol\'{i}tica, a escolha da- queles que tomar\~{a}o as decis\~{o}es. Mas uma democracia representativa mais pr\'{o}xima do ideal de soberania popular deveria contemplar a se- gunda dimens\~{a}o – a forma\c{c}\~{a}o da agenda, hoje fortemente determina- da pelos meios de comunica\c{c}\~{a}o de massa. Portanto, cabe entender a m\'{i}dia como sendo tamb\'{e}m um es- pa\c{c}o de representa\c{c}\~{a}o pol\'{i}tica. E o reconhecimento da terceira dimen- s\~{a}o implica a necessidade de gera- \c{c}\~{a}o de espa\c{c}os que permitam aos grupos subalternos formularem au- tonomamente seus interesses, isto \'{e}, de uma sociedade civil desenvolvida e plural.},
  File                     = {:home/haydee/\'{A}rea de trabalho/Ref\_bib/Miguel\_2003.pdf:pdf},
  Keywords                 = {FAFICH,agenda p\'{u}blica,democracia,monografia,m\'{i}dia,representacao,representa\c{c}\~{a}o pol\'{i}tica,sociedade civil},
  Mendeley-tags            = {FAFICH,democracia,monografia,representacao},
  Url                      = {http://www.scielo.br/pdf/rbcsoc/v18n51/15989}
}

