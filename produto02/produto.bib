@Article{MARQUES2010a,
  Title                    = {{Participa\c{c}\~{a}o, institui\c{c}\~{o}es pol\'{i}ticas e Internet: um exame dos canais participativos nos portais da C\^{a}mara e da Presid\^{e}ncia do Brasil}},
  Author                   = {Marques, Francisco Paulo Jamil Almeida},
  Journal                  = {Intercom-Revista Brasileira de Ci\^{e}ncias da Comunica\c{c}\~{a}o},
  Year                     = {2010},
  Pages                    = {53--79},
  Volume                   = {33},

  Abstract                 = {Este artigo investiga como duas institui\c{c}\~{o}es do Estado brasileiro - a Presid\^{e}ncia da Rep\'{u}blica e a C\^{a}mara dos Deputados - oferecem, por meio da Internet, ca- nais voltados \`{a} promo\c{c}\~{a}o da participa\c{c}\~{a}o pol\'{i}tica dos cidad\~{a}os. Ap\'{o}s revis\~{a}o da literatura a tratar da interface entre Internet e Democracia, dedica-se especial cuidado ao invent\'{a}rio e \`{a} descri\c{c}\~{a}o dos mecanismos participativos encontrados nestas duas experi\^{e}ncias, bem como reflete-se acerca de seus poss\'{i}veis efeitos sobre o processo de produ\c{c}\~{a}o da decis\~{a}o pol\'{i}tica. No caso da C\^{a}mara, identifica- se, em compara\c{c}\~{a}o com o Portal da Presid\^{e}ncia, maior variedade de canais a concretizarem a participa\c{c}\~{a}o. Logo, a natureza da intera\c{c}\~{a}o que a esfera civil mant\'{e}m com esta institui\c{c}\~{a}o tende a ser diferenciada. Tamb\'{e}m s\~{a}o discutidos outros aspectos a influenciarem a oferta destes mecanismos, tais como seus custos pol\'{i}ticos. Refor\c{c}a-se, ao final, a necessidade de se pensar os meios digitais em interface com o contexto cultural e pol\'{i}tico de cada sociedade democr\'{a}tica.},
  File                     = {:home/haydee/\'{A}rea de trabalho/Ref\_bib/Marques\_2010(2).pdf:pdf},
  Keywords                 = {FAFICH,democracia,institui\c{c}\~{o}es,internet,monografia,participa\c{c}\~{a}o,pol\'{i}ticas},
  Mendeley-tags            = {FAFICH,monografia},
  Url                      = {http://200.144.189.84/revistas/index.php/revistaintercom/article/viewArticle/147}
}

@Article{MARQUES2010b,
  Title                    = {{"Muro baixo, o povo pula": iniciativas institucionais de participa\c{c}\~{a}o digital e seus desafios fundamentais}},
  Author                   = {Marques, Francisco Paulo Jamil Almeida},
  Journal                  = {Opini\~{a}o P\'{u}blica},
  Year                     = {2010},
  Number                   = {1},
  Pages                    = {117--142},
  Volume                   = {16},

  Abstract                 = {O artigo investiga as dificuldades mais comuns enfrentadas pelos projetos governamentais voltados para incrementar a participa\c{c}\~{a}o atrav\'{e}s do emprego dos media digitais. Ap\'{o}s breve diagn\'{o}stico dos recursos participativos existentes nos Portais da Presid\^{e}ncia da Rep\'{u}blica e da C\^{a}mara dos Deputados, \'{e} verificado se os constrangimentos t\'{e}cnicos e pol\'{i}ticos que a literatura aponta em rela\c{c}\~{a}o \`{a} ado\c{c}\~{a}o das ferramentas de comunica\c{c}\~{a}o digital se refletem em tais experi\^{e}ncias. \`{A} luz dos elementos fornecidos pela investiga\c{c}\~{a}o dos casos emp\'{i}ricos, defende-se o argumento de que promover a participa\c{c}\~{a}o requer mais do que a oferta de recursos de comunica\c{c}\~{a}o, uma vez que cultura c\'{i}vica e outras peculiaridades de cada democracia s\~{a}o fatores determinantes a influenciarem os padr\~{o}es de envolvimento pol\'{i}tico. Se a id\'{e}ia dos autores mais euf\'{o}ricos se refere ao argumento de que a Internet torna o ``muro'' que protege as institui\c{c}\~{o}es representativas mais f\'{a}cil de pular, deve-se considerar que o jogo pol\'{i}tico tradicional conta, muitas vezes, com artif\'{i}cios pr\'{o}prios a fim de evitar o emprego efetivo das capacidades da rede digital.},
