@MastersThesis{SANTOS2006,
  Title                    = {Desigualdade social e inclus\~{a}o digital no Bras},
  Author                   = {Santos, Edvalter Souza},
  School                   = {IPPUR/UFRJ},
  Year                     = {2006},

  Abstract                 = {O desenvolvimento das Tecnologias da Informa\c{c}\~{a}o e da Comunica\c{c}\~{a}o (TIC) e sua desigual apropria\c{c}\~{a}o pelos diferentes estratos sociais originaram um novo tipo de desigualdade, cuja face mais vis\'{i}vel (n\~{a}o, talvez, a mais relevante) \'{e} a impossibilidade de acesso dos mais pobres ao computador, \`{a} internet e aos conhecimentos b\'{a}sicos de inform\'{a}tica. Trata-se da desigualdade digital (para alguns, exclus\~{a}o digital; nos EUA, digital divide; na Argentina, brecha digital ). Para combatê-la, o Governo e seus parceiros da sociedade civil e do mercado anunciam uma pol\'{i}tica de inclus\~{a}o digital universal autonomista e cidad\~{a}, isto \'{e}, garantia de treinamento b\'{a}sico e de acesso \`{a}s TIC para toda a popula\c{c}\~{a}o, apoiada num modelo pedag\'{o}gico capaz de promover o desenvolvimento da autonomia e da competência/motiva\c{c}\~{a}o para a participa\c{c}\~{a}o cidad\~{a} do sujeito. O projeto prevê a implanta\c{c}\~{a}o de laborat\'{o}rios de inform\'{a}tica ligados \`{a} internet nas escolas p\'{u}blicas, e a cria\c{c}\~{a}o de telecentros p\'{u}blicos gratuitos para a popula\c{c}\~{a}o pobre fora-da-escola. Contudo, nossa pesquisa encontrou escolas pobremente equipadas, contato muito reduzido do aluno com o laborat\'{o}rio, falta de manuten\c{c}\~{a}o das m\'{a}quinas e insuficiência de professores capacitados. O n\'{u}mero de telecentros \'{e} irris\'{o}rio, quando mais da metade dos brasileiros (54,79\%) nunca usou computadores e 67,76\% jamais entrou na internet. E, a pedagogia da “autonomiza\c{c}\~{a}o” e da “conscientiza\c{c}\~{a}o cidad\~{a}” requer intera\c{c}\~{a}o bem maior que o pobre contato instrutor/aluno no telecentro. Assim, a pol\'{i}tica anunciada est\'{a} sob quest\~{a}o. Contudo, um projeto mais honesto precisa ser tocado: s\~{a}o os contornos desta “inclus\~{a}o poss\'{i}vel” que nos propomos discutir. Conclu\'{i}mos que a inclus\~{a}o digital universal autonomista-cidad\~{a} n\~{a}o faz parte dos horizontes da d\'{e}cada, mas a “inclus\~{a}o poss\'{i}vel” \'{e} necess\'{a}ria. O caminho mais apropriado para a universaliza\c{c}\~{a}o em m\'{e}dio prazo \'{e} a informatiza\c{c}\~{a}o da escola p\'{u}blica. \'{E} urgente fazer de toda escola uma escola conectada. Os demais “exclu\'{i}dos digitais” devem ser atendidos atrav\'{e}s da implanta\c{c}\~{a}o de telecentros p\'{u}blicos. Mas, deve ser criada tamb\'{e}m um a solu\c{c}\~{a}o de parceria com “telecentros privados”, para atingir uma escala num\'{e}rica que as instala\c{c}\~{o}es p\'{u}blicas jamais poder\~{a}o alcan\c{c}ar.},
  Comment                  = {Acesso em 30 de setembro de 2014},
  Owner                    = {haydee},
  Timestamp                = {2014.10.07},
  Url                      = {http://www.ippur.ufrj.br/download/pub/EdvalterSouzaSantos-tese.pdf}
}

